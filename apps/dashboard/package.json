    "@hello-pangea/dnd": "^16.5.0",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@stripe/stripe-js": "^2.4.0",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-table": "^8.11.3",
    "@tiptap/pm": "^2.1.16",
